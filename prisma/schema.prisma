generator client {
    provider = "prisma-client-js"
    output   = "../src/lib/generated/prisma-client"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

// School model
model School {
    id        String @id @default(cuid())
    name      String
    address   String
    latitude  Float
    longitude Float
}
